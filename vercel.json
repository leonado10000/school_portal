{
    "version": 2,
    "builds": [{
        "src": "school_portal/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "school_portal/wsgi.py"
        }
    ],
    "build": {
    "env": {
      "PIP_ONLY_BINARY": ":all:",
      "PIP_NO_BUILD_ISOLATION": "false"
    }
  }
}
